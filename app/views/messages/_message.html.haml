.main__message__up{ data: { id: "#{message.id}" } }
  .main__message__up__name
    = message.user.name
  .main__message__up__date
    = message.created_at.strftime('%Y/%m/%d %H:%M')
- if message.body?
  .main__message__low
    = message.body
- if message.image?
  .main__message__low
    = image_tag "#{ message.image.thumb }"
%br
